parameters:
    level: 4
    ignoreErrors:
        - '~^Method Doctrine\\DBAL(\\.*)?Connection::query\(\) invoked with \d+ parameters?, 0 required\.\z~'
    paths:
        - src
    bootstrapFiles:
        - phpstan-bootstrap.php
    stubFiles:
        - stubs/Asset.stub
        - stubs/Pdf.stub
includes:
    - phpstan-baseline.neon
